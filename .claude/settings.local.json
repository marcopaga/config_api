{
  "permissions": {
    "allow": [
      "Bash(mix test:*)",
      "Bash(docker-compose ps:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(mix deps.get:*)",
      "Bash(mix compile:*)",
      "Bash(docker-compose up:*)",
      "Bash(MIX_ENV=dev mix event_store.create:*)",
      "Bash(MIX_ENV=dev mix event_store.init:*)",
      "Bash(MIX_ENV=test mix event_store.init:*)"
    ]
  },
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true
  }
}
